    "@types/findup-sync": "4.0.2",
    "@types/fs-extra": "9.0.13",
    "@types/rimraf": "3.0.2",
    "babel-plugin-tester": "10.1.0",
    "graphql-tag": "2.12.6",
    "jest": "29.3.1"
  },
